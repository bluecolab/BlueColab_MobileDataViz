{
  "name": "aquawatchmobile",
  "version": "1.9.5",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "postinstall": "patch-package",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "tsc --noEmit && eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0 && prettier -c \"**/*.{js,jsx,ts,tsx}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx}\" --write",
    "test": "npm run lint && npx cspell . && npm audit --audit-level=low && npx expo-doctor"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-orientation-locker",
          "react-native-navigation-bar",
          "react-navigation",
          "react-navigation-stack",
          "tailwindcss",
          "@babel/core"
        ]
      }
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.4",
    "@react-navigation/native": "^7.1.8",
    "@shopify/react-native-skia": "2.4.18",
    "@tanstack/react-query": "5.90.3",
    "@types/react": "19.0.10",
    "axios": "1.13.5",
    "babel-preset-expo": "~55.0.5",
    "date-fns": "4.1.0",
    "expo": "~55.0.3",
    "expo-constants": "~55.0.2",
    "expo-font": "~55.0.2",
    "expo-linear-gradient": "~55.0.8",
    "expo-linking": "~55.0.7",
    "expo-location": "~55.1.2",
    "expo-network": "~55.0.8",
    "expo-router": "~55.0.2",
    "expo-splash-screen": "~55.0.9",
    "expo-status-bar": "~55.0.4",
    "expo-system-ui": "~55.0.9",
    "expo-web-browser": "~55.0.9",
    "lottie-react-native": "~7.3.4",
    "nativewind": "4.2.1",
    "react": "19.2.0",
    "react-native": "0.83.2",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-picker-select": "9.3.1",
    "react-native-reanimated": "4.2.1",
    "react-native-reanimated-carousel": "4.0.2",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-webview": "13.16.0",
    "react-native-worklets": "0.7.2",
    "victory-native": "41.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.2.2",
    "@typescript-eslint/eslint-plugin": "8.39.0",
    "@typescript-eslint/parser": "8.39.0",
    "cspell": "9.2.1",
    "eslint": "9.32.0",
    "eslint-config-expo": "~55.0.0",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.3.0",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.2"
  },
  "private": false
}