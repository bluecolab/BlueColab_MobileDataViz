{
  "name": "aquawatchmobile",
  "version": "1.8.5",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "postinstall": "patch-package",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0 && prettier -c \"**/*.{js,jsx,ts,tsx}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx}\" --write",
    "test": "npm run lint && npx expo-doctor && npx cspell . && npm audit --audit-level=low"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-element-dropdown",
          "react-native-orientation-locker",
          "react-native-navigation-bar",
          "react-navigation",
          "react-navigation-stack",
          "tailwindcss",
          "luxon",
          "@babel/core"
        ]
      }
    }
  },
  "overrides": {
    "@shopify/react-native-skia": "2.0.0-next.4",
    "minimatch@9.0.5": {
      "brace-expansion": "2.0.2"
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "2.0.0-next.4",
    "@types/react": "~19.0.10",
    "axios": "^1.10.0",
    "expo": "53.0.20",
    "expo-constants": "~17.1.6",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.6",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "luxon": "^3.6.1",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-webview": "13.13.5",
    "victory-native": "^41.17.3",
    "@react-native-picker/picker": "2.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/luxon": "^3.6.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "cspell": "^9.1.3",
    "eslint": "^9.29.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "private": false
}