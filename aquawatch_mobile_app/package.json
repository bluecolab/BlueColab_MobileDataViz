{
  "name": "aquawatchmobile",
  "version": "1.9.0-expo-54-alpha",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo start --android",
    "postinstall": "patch-package",
    "ios": "expo start --ios",
    "start": "expo start",
    "prebuild": "expo prebuild",
    "lint": "tsc --noEmit && eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings=0 && prettier -c \"**/*.{js,jsx,ts,tsx}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx}\" --write",
    "test": "npm run lint && npx expo-doctor && npx cspell . && npm audit --audit-level=low"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-element-dropdown",
          "react-native-orientation-locker",
          "react-native-navigation-bar",
          "react-navigation",
          "react-navigation-stack",
          "tailwindcss",
          "@babel/core"
        ]
      }
    }
  },
  "overrides": {
    "@shopify/react-native-skia": {
      "react-native-reanimated": ">3.0"
    }
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^7.1.6",
    "@shopify/react-native-skia": "2.2.3",
    "@types/react": "~19.0.10",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "expo": "^54.0.0-preview.6",
    "expo-constants": "^18.0.3",
    "expo-font": "~14.0.2",
    "expo-linear-gradient": "~15.0.2",
    "expo-linking": "^8.0.3",
    "expo-location": "~19.0.2",
    "expo-network": "~8.0.2",
    "expo-router": "~6.0.0-beta.5",
    "expo-splash-screen": "^31.0.4",
    "expo-status-bar": "~3.0.3",
    "expo-system-ui": "~6.0.2",
    "expo-web-browser": "~15.0.2",
    "lottie-react-native": "~7.3.1",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-picker-select": "^9.3.1",
    "react-native-reanimated": "~4.0.2",
    "react-native-reanimated-carousel": "^4.0.2",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.14.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "~0.4.1",
    "victory-native": "^41.19.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "cspell": "^9.1.3",
    "eslint": "^9.32.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-typescript": "^4.4.3",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": false
}
